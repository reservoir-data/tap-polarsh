  "replication_method": "FULL_TABLE",
  "schema": {
    "properties": {
      "allow_customer_updates": {
        "description": "Whether customers can update their subscriptions from the customer portal.",
        "title": "Allow Customer Updates",
        "type": "boolean"
      },
      "avatar_url": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ],
        "description": "Avatar URL shown in checkout, customer portal, emails etc.",
        "title": "Avatar Url"
      },
      "created_at": {
        "description": "Creation timestamp of the object.",
        "format": "date-time",
        "title": "Created At",
        "type": "string"
      },
      "customer_email_settings": {
        "description": "Settings related to customer emails",
        "properties": {
          "order_confirmation": {
            "title": "Order Confirmation",
            "type": "boolean"
          },
          "subscription_cancellation": {
            "title": "Subscription Cancellation",
            "type": "boolean"
          },
          "subscription_confirmation": {
            "title": "Subscription Confirmation",
            "type": "boolean"
          },
          "subscription_cycled": {
            "title": "Subscription Cycled",
            "type": "boolean"
          },
          "subscription_past_due": {
            "title": "Subscription Past Due",
            "type": "boolean"
          },
          "subscription_revoked": {
            "title": "Subscription Revoked",
            "type": "boolean"
          },
          "subscription_uncanceled": {
            "title": "Subscription Uncanceled",
            "type": "boolean"
          },
          "subscription_updated": {
            "title": "Subscription Updated",
            "type": "boolean"
          }
        },
        "required": [
          "order_confirmation",
          "subscription_cancellation",
          "subscription_confirmation",
          "subscription_cycled",
          "subscription_past_due",
          "subscription_revoked",
          "subscription_uncanceled",
          "subscription_updated"
        ],
        "title": "OrganizationCustomerEmailSettings",
        "type": "object"
      },
      "details_submitted_at": {
        "anyOf": [
          {
            "format": "date-time",
            "type": "string"
          },
          {
            "type": "null"
          }
        ],
        "description": "When the business details were submitted.",
        "title": "Details Submitted At"
      },
      "email": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ],
        "description": "Public support email.",
        "title": "Email"
      },
      "feature_settings": {
        "anyOf": [
          {
            "properties": {
              "issue_funding_enabled": {
                "default": false,
                "description": "If this organization has issue funding enabled",
                "title": "Issue Funding Enabled",
                "type": "boolean"
              },
              "revops_enabled": {
                "default": false,
                "description": "If this organization has RevOps enabled",
                "title": "Revops Enabled",
                "type": "boolean"
              },
              "seat_based_pricing_enabled": {
                "default": false,
                "description": "If this organization has seat-based pricing enabled",
                "title": "Seat Based Pricing Enabled",
                "type": "boolean"
              },
              "wallets_enabled": {
                "default": false,
                "description": "If this organization has Wallets enabled",
                "title": "Wallets Enabled",
                "type": "boolean"
              }
            },
            "title": "OrganizationFeatureSettings",
            "type": "object"
          },
          {
            "type": "null"
          }
        ],
        "description": "Organization feature settings"
      },
      "id": {
        "description": "The ID of the object.",
        "format": "uuid4",
        "title": "Id",
        "type": "string"
      },
      "modified_at": {
        "anyOf": [
          {
            "format": "date-time",
            "type": "string"
          },
          {
            "type": "null"
          }
        ],
        "description": "Last modification timestamp of the object.",
        "title": "Modified At"
      },
      "name": {
        "description": "Organization name shown in checkout, customer portal, emails etc.",
        "title": "Name",
        "type": "string"
      },
      "notification_settings": {
        "description": "Settings related to notifications",
        "properties": {
          "new_order": {
            "title": "New Order",
            "type": "boolean"
          },
          "new_subscription": {
            "title": "New Subscription",
            "type": "boolean"
          }
        },
        "required": [
          "new_order",
          "new_subscription"
        ],
        "title": "OrganizationNotificationSettings",
        "type": "object"
      },
      "proration_behavior": {
        "description": "Proration behavior applied when customer updates their subscription from the portal.",
        "enum": [
          "invoice",
          "prorate"
        ],
        "title": "SubscriptionProrationBehavior",
        "type": "string"
      },
      "slug": {
        "description": "Unique organization slug in checkout, customer portal and credit card statements.",
        "title": "Slug",
        "type": "string"
      },
      "socials": {
        "description": "Links to social profiles.",
        "items": {
          "properties": {
            "platform": {
              "description": "The social platform of the URL",
              "enum": [
                "x",
                "github",
                "facebook",
                "instagram",
                "youtube",
                "tiktok",
                "linkedin",
                "other"
              ],
              "title": "OrganizationSocialPlatforms",
              "type": "string"
            },
            "url": {
              "description": "The URL to the organization profile",
              "format": "uri",
              "maxLength": 2083,
              "minLength": 1,
              "title": "Url",
              "type": "string"
            }
          },
          "required": [
            "platform",
            "url"
          ],
          "title": "OrganizationSocialLink",
          "type": "object"
        },
        "title": "Socials",
        "type": "array"
      },
      "status": {
        "description": "Current organization status",
        "enum": [
          "created",
          "onboarding_started",
          "initial_review",
          "ongoing_review",
          "denied",
          "active"
        ],
        "title": "OrganizationStatus",
        "type": "string"
      },
      "subscription_settings": {
        "description": "Settings related to subscriptions management",
        "properties": {
          "allow_customer_updates": {
            "title": "Allow Customer Updates",
            "type": "boolean"
          },
          "allow_multiple_subscriptions": {
            "title": "Allow Multiple Subscriptions",
            "type": "boolean"
          },
          "benefit_revocation_grace_period": {
            "title": "Benefit Revocation Grace Period",
            "type": "integer"
          },
          "prevent_trial_abuse": {
            "title": "Prevent Trial Abuse",
            "type": "boolean"
          },
          "proration_behavior": {
            "enum": [
              "invoice",
              "prorate"
            ],
            "title": "SubscriptionProrationBehavior",
            "type": "string"
          }
        },
        "required": [
          "allow_multiple_subscriptions",
          "allow_customer_updates",
          "proration_behavior",
          "benefit_revocation_grace_period",
          "prevent_trial_abuse"
        ],
        "title": "OrganizationSubscriptionSettings",
        "type": "object"
      },
      "website": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ],
        "description": "Official website of the organization.",
        "title": "Website"
      }
    },
    "required": [
      "created_at",
      "modified_at",
      "id",
      "name",
      "slug",
      "avatar_url",
      "proration_behavior",
      "allow_customer_updates",
      "email",
      "website",
      "socials",
      "status",
      "details_submitted_at",
      "feature_settings",
      "subscription_settings",
      "notification_settings",
      "customer_email_settings"
    ],
    "title": "Organization",
    "type": "object"
  },
  "stream": "organizations",
  "tap_stream_id": "organizations"
}
